@model IEnumerable<thuctap_CTN.Models.Chitietbaigiang>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Bg)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.L)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Bg.BgId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.L.LId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CtbgId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CtbgId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CtbgId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
