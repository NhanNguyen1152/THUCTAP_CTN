@model IEnumerable<thuctap_CTN.Models.Taikhoan>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TkUrn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TkPw)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TkUrn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TkPw)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TkId">Edit</a> |
